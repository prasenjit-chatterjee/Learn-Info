<h3>Delete view</h3>

<div class="row">
    <div class="col-md-6">
        <label>Id:</label>
        <label class="form-control" ng-model="Product.id">{{Product.id}}</label>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <label>Name:</label>
        <label class="form-control" ng-model="Product.name">{{Product.name}}</label>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <label>Description:</label>
        <label class="form-control" ng-model="Product.description">{{Product.description}}</label>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <label>Quantity:</label>
        <label class="form-control" ng-model="Product.quantity">{{Product.quantity}}</label>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <input type="button" value="Delete" class="btn" data-ng-click="delete()" />
    </div>
</div>